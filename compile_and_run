clang -flto -c -o input.bc input.c
opt -instnamer -o input.instnamer.bc input.bc
#gdb -x gdbCommands.txt --args klee -solver-backend=stp -output-module -output-source -use-query-log=all:pc,all:smt2,solver:smt2,solver:pc ./input.instnamer.bc 
klee  --libc=uclibc --posix-runtime -solver-backend=metasmt -output-module -output-source -use-query-log=all:smt2,solver:smt2 ./input.instnamer.bc
llvm-dis -o=input.ll ./klee-last/final.bc
